<h1>ARTICLES LISTING PAGE</h1>

<table>
<thead>
  <tr>
    <th>Title</th>
    <th>Body</th>
    <th>Actions</th>
  </tr>
</thead>
<tbody>
  <% @articles.each do |article| %>
    <tr>
      <td><%= article.title %> </td>
      <td><%= article.body %> </td>
      <td>Actions</td>
    <tr>
  <% end %>
</tbody>
</table>
<%= link_to 'New article',  new_article_path %>